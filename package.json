{
  "name": "native-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "reset-project": "node ./scripts/reset-project.js",
    "lint": "expo lint",
    "build:dev": "eas build --profile development --platform all",
    "build:preview": "eas build --profile preview --platform all",
    "build:prod": "eas build --profile production --platform all",
    "submit:android": "eas submit -p android --latest",
    "submit:ios": "eas submit -p ios --latest",
    "ci:fix": "bun run ci:check:fix && bun run ci:format:fix && bun run ci:lint:fix",
    "ci:check:fix": "biome check --fix",
    "ci:format": "biome format",
    "ci:format:fix": "biome format --fix",
    "ci:check": "biome check",
    "ci:lint": "biome lint",
    "ci:lint:fix": "biome lint --fix"
  },
